@model MvcLibrary.Models.Entity.Member
@{
    ViewBag.Title = "MemberAdd";
    Layout = "~/Views/Shared/Layout.cshtml";
}







<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/4.0.0/jquery.validate.unobtrusive.min.js"></script>



<script src="~/Scripts/jquery-3.7.0.js"></script>

<br />
<form class="form-group" method="post" id="authorForm">
    <div>
        <label>Üye Adı</label>
        @Html.TextBoxFor(x => x.MemberName, new { @class = "form-control" })
        <br />
        <label>Üye Soyadı</label>
        @Html.TextBoxFor(x => x.MemberSurname, new { @class = "form-control" })
        <br />
        <label>Üye Mail</label>
        @Html.TextBoxFor(x => x.MemberMail, new { @class = "form-control" })
        <br />
        <label>Üye Kullanıcı Adı</label>
        @Html.TextBoxFor(x => x.Username, new { @class = "form-control" })
        <br />
        <label>Üye Şifresi</label>
        @Html.TextBoxFor(x => x.Password, new { @class = "form-control" })
        <br />
        <label>Üye Resmi</label>
        @Html.TextBoxFor(x => x.Image, new { @class = "form-control" })
        <br />
        <label>Üye Telefonu</label>
        @Html.TextBoxFor(x => x.Telephone, new { @class = "form-control" })
        <br />
        <label>Üye Okulu</label>
        @Html.TextBoxFor(x => x.School, new { @class = "form-control" })
        <br />
    </div>

    <div>
        <br />
        <button class="btn btn-outline-success" type="submit" id="btn1">Ekle</button>
    </div>
</form>





